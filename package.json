{
  "name": "xombi",
  "version": "2.3.0-alpha.0",
  "description": "An XMTP bot used to handle requests for Ombi",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "npx tsx index.ts",
    "test": "npx jest --config jest.config.cjs --coverage",
    "format": "npx prettier . --write",
    "lint": "npx eslint .",
    "typecheck": "npx tsc --noEmit"
  },
  "repository": {
